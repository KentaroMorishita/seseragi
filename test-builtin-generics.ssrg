type ID = String
type Hoge<T> = T
type Fuga<T> = Hoge<T>

type Option<T> = Maybe<T>

fn f x: Fuga<ID> -> String = `${x}`
fn g x: Option<ID> -> String = if true then "test" else "test"

// 正しい型での使用例
show $ f "hoge"           // String -> String (OK)
show $ g (Just "world")   // Maybe<String> -> String (OK)
show $ g Nothing          // Maybe<String> -> String (OK)