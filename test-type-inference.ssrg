// Test file for Maybe type inference
fn safeDivide x: Int -> y: Int -> Maybe<Int> = if y == 0
  then Nothing
  else Just (x / y)

let result1 = safeDivide 10 2
let result2 = safeDivide 10 0

let justValue = Just 42
let nothingValue = Nothing

print result1
print result2
print justValue
print nothingValue