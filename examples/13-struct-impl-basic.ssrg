// 構造体とimplブロックのテスト

struct Point {
  x: Int,
  y: Int
}

impl Point {
  fn add self: Point -> other: Point -> Point =
    Point { x: self.x + other.x, y: self.y + other.y }

  fn distanceSquared self: Point -> Int =
    self.x * self.x + self.y * self.y
}

let p1 = Point { x: 3, y: 4 }
let p2 = Point { x: 1, y: 2 }

show p1
show p2