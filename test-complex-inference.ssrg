// 複雑な型推論のテスト
fn add x: Int -> y: Int -> Int = x + y
fn double x: Int -> Int = x * 2

let sum = add 10 5
let doubled = double 7
let message = "Hello, World!"
let number = 42

print sum
print doubled
print message  
print number

// Maybe型のテスト
