// メソッド呼び出しとホバー表示のテスト

struct Point {
  x :Int,
  y :Int
}

impl Point {
  fn add self -> other :Point -> Point =
    Point { x: self.x + other.x, y: self.y + other.y }
}

let p1 = Point { x: 3, y: 4 }
let p2 = Point { x: 1, y: 2 }

// これらは同じ型情報を表示すべき
let p3 = p1 add p2   // メソッド呼び出し
let p4 = p1 + p2     // 演算子オーバーロード